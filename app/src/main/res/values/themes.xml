<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.TwdLauncher_net" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>
    <style name="AppTheme.Fullscreen" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- 隐藏状态栏 -->
        <item name="android:windowFullscreen">true</item>
        <!-- 延伸布局到导航栏区域 -->
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
    <style name="Theme.TwdLauncher_net" parent="AppTheme.Fullscreen">
        <item name="android:windowBackground">@color/black</item>
    </style>

    <style name="Theme.Index_Standard" parent="Theme.TwdLauncher_net">
        <item name="indexPadding">@dimen/index_padding_normal</item>
        <item name="indexMargin">@dimen/index_margin_normal</item>
    </style>

    <style name="Theme.Index_Yameixun" parent="Theme.TwdLauncher_net">
        <item name="indexPadding">@dimen/index_padding_yameixun</item>
        <item name="indexMargin">@dimen/index_margin_yameixun</item>
    </style>
</resources>